TARGET_URL = "http://localhost:8000/register"   # ğŸ›  æ›¿æ¢æˆç›®æ ‡æ³¨å†Œæ¥å£
THREADS = 20                                     # å¹¶å‘çº¿ç¨‹æ•°
TOTAL_REQUESTS = 500                             # æ€»æ³¨å†Œæ¬¡æ•°
# æ¨¡æ‹Ÿçš„è¯·æ±‚æ•°æ®æ ¼å¼ï¼ˆæŒ‰éœ€è°ƒæ•´å‚æ•°åï¼‰
def gen_form():
    username = "user_" + ''.join(random.choices(string.ascii_letters + string.digits, k=6))
    email = username + "@test.com"
    return {
        "username": username,
        "email": email,
        "password": "123456",
        "confirm": "123456"
    }
# ========================================================

def register(index):
    try:
        r = requests.post(TARGET_URL, data=gen_form(), timeout=5)
        if r.status_code == 200:
            print(f"[{index}] âœ… æ³¨å†ŒæˆåŠŸ")
        else:
            print(f"[{index}] âŒ å¤±è´¥ï¼ŒçŠ¶æ€ç ï¼š{r.status_code}")
    except Exception as e:
        print(f"[{index}] âŒ é”™è¯¯ï¼š{str(e)}")

def run():
    print(f"[*] å¼€å§‹ç–¯ç‹‚æ³¨å†Œï¼š{TOTAL_REQUESTS} æ¬¡ï¼Œ{THREADS} å¹¶å‘çº¿ç¨‹...")
    with ThreadPoolExecutor(max_workers=THREADS) as executor:
        for i in range(TOTAL_REQUESTS):
            executor.submit(register, i+1)

if __name__ == "__main__":
    run()
